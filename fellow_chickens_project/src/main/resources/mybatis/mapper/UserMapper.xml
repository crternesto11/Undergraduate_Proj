<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fellowchicken.mapper.UserMapper">
    <insert id="add" parameterType="com.fellowchicken.model.User" useGeneratedKeys="true" keyProperty="id">
        insert into users(UserName, Password, Email, Phone)
        values (#{user.userName}, #{user.password}, #{user.email}, #{user.phone})
    </insert>
</mapper>